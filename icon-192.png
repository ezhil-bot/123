  </span><span><span class="hljs-comment">// Prevent autofocus warning by removing focus before proceeding</span></span><span>
  </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-property">activeElement</span></span><span>.</span><span><span class="hljs-title function_">blur</span></span><span>();

  </span><span><span class="hljs-keyword">const</span></span><span> role = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">'role'</span></span><span>).</span><span><span class="hljs-property">value</span></span><span>;
  </span><span><span class="hljs-keyword">const</span></span><span> id = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">'id'</span></span><span>).</span><span><span class="hljs-property">value</span></span><span>.</span><span><span class="hljs-title function_">trim</span></span><span>();

  </span><span><span class="hljs-keyword">if</span></span><span> (!id) {
    </span><span><span class="hljs-title function_">alert</span></span><span>(</span><span><span class="hljs-string">"Please enter your ID."</span></span><span>);
