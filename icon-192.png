</span><span><span class="hljs-comment">// Install: cache essential files</span></span><span>
self.</span><span><span class="hljs-title function_">addEventListener</span></span><span>(</span><span><span class="hljs-string">'install'</span></span><span>, </span><span><span class="hljs-function"><span class="hljs-params">event</span></span></span><span> =&gt; {
