</span><span><span class="hljs-keyword">import</span></span><span> os

os.makedirs(</span><span><span class="hljs-string">'screenshots'</span></span><span>, exist_ok=</span><span><span class="hljs-literal">True</span></span><span>)

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">create_placeholder</span></span><span>(</span><span><span class="hljs-params">text, filename</span></span><span>):
    img = Image.new(</span><span><span class="hljs-string">'RGB'</span></span><span>, (</span><span><span class="hljs-number">300</span></span><span>, </span><span><span class="hljs-number">200</span></span><span>), color=(</span><span><span class="hljs-number">220</span></span><span>, </span><span><span class="hljs-number">230</span></span><span>, </span><span><span class="hljs-number">250</span></span><span>))
    d = ImageDraw.Draw(img)
    </span><span><span class="hljs-keyword">try</span></span><span>:
        font = ImageFont.truetype(</span><span><span class="hljs-string">"arial.ttf"</span></span><span>, </span><span><span class="hljs-number">30</span></span><span>)
    </span><span><span class="hljs-keyword">except</span></span><span>:
        font = ImageFont.load_default()
    w, h = d.textsize(text, font=font)
    d.text(((</span><span><span class="hljs-number">300</span></span><span> - w)/</span><span><span class="hljs-number">2</span></span><span>, (</span><span><span class="hljs-number">200</span></span><span> - h)/</span><span><span class="hljs-number">2</span></span><span>), text, fill=(</span><span><span class="hljs-number">50</span></span><span>, </span><span><span class="hljs-number">70</span></span><span>, </span><span><span class="hljs-number">160</span></span><span>), font=font)
    img.save(</span><span><span class="hljs-string">f'screenshots/<span class="hljs-subst">{filename}</span></span></span><span>')

create_placeholder(</span><span><span class="hljs-string">"Login Screen"</span></span><span>, </span><span><span class="hljs-string">"login.png"</span></span><span>)
create_placeholder(</span><span><span class="hljs-string">"Doctor Dashboard"</span></span><span>, </span><span><span class="hljs-string">"doctor.png"</span></span><span>)
